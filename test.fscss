$primary-color:midnightblue;
$Border-stk: 3px solid #59f;
$(name: box){
%2(width, height[:100px;])
Border: $Border-stk!;
Background-color: mx(#,'e');
}
re(animationSettings, '3s linear infinite alternate forwards') 

$(@keyframes animate, div, $(name: box) &[animationSettings]){
0%{ rotate: 0;} 
100%{rotate: 360deg;} 
} 

